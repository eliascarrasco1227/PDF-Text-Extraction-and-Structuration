                        <form>ch'ut-a'q</form>
                        <gloss>pequeño-PL</gloss>
                    </unit>
                    <unit>
                        <form>xtan-i'...</form>
                        <gloss>niña-PL</gloss>
                    </unit>
                </morpheme_analysis>
            </interlinear_gloss>
        </seccion>
        <seccion type="Modificadores del adjetivo">
